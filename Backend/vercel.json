{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "Backend/proxyserver.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "Backend/proxyserver.js"
        },
        {
            "src": "/(.*)",
            "dest": "/FoodOrdering.html"
        }
    ]
}
